    <!-- http://www.pizza.owl/antologies/PIZZA_0000024 -->

    <owl:Class rdf:about="http://www.pizza.owl/antologies/PIZZA_0000024">
        <antologies:PIZZA_0000103></antologies:PIZZA_0000103>
        <antologies:PIZZA_0000103 xml:lang="en">Hogwarts School of Witchcraft and Wizardry, shortened to Hogwarts, is a fictional British school of magic for students aged eleven to eighteen, and is the primary setting for the first six books in J.K. Rowling&apos;s Harry Potter series.  - https://en.wikipedia.org/wiki/Hogwarts</antologies:PIZZA_0000103>
        <rdfs:label xml:lang="en">Hogwarts_Castle</rdfs:label>
    </owl:Class>
    


