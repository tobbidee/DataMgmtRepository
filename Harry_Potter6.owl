        <antologies:PIZZA_0000103 xml:lang="en">A character is a person or other being in a narrative work of art. 
https://en.wikipedia.org/wiki/Character_(arts)</antologies:PIZZA_0000103>
        <rdfs:label xml:lang="en">Characters</rdfs:label>
    </owl:Class>
    


    <!-- http://www.pizza.owl/antologies/PIZZA_0000026 -->

    <owl:Class rdf:about="http://www.pizza.owl/antologies/PIZZA_0000026">
        <rdfs:subClassOf rdf:resource="http://www.pizza.owl/antologies/PIZZA_0000024"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.pizza.owl/antologies/PIZZA_0000129"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.pizza.owl/antologies/PIZZA_0000043"/>
